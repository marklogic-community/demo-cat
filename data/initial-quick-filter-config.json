{
   "_comment":"Refer to documentation at https://docs.marklogic.com/guide/search-dev/structured-query#id_59265",
   "quickFilter":[
      {
         "label":"Live Site Demo",
         "query":{
            "or-query":{
              "queries": [
                {
                  "value-query":{
                   "type":"string",
                   "json-property":"urlType",
                   "text":[
                      "external"
                   ]
                  }
                },
                {
                  "value-query":{
                   "type":"string",
                   "json-property":"name",
                   "text":[
                      "MarkMail"
                   ]
                  }
                }
              ]
            }
         }
      },
      {
         "label":"No Restrictions",
         "query":{
            "not-query":{
               "or-query":{
                  "queries":[
                     {
                        "value-query":{
                           "type":"boolean",
                           "json-property":"restricted",
                           "text":[
                              true
                           ]
                        }
                     },
                     {
                        "value-query":{
                           "type":"string",
                           "json-property":"accessibility",
                           "text":[
                              "requires VPN"
                           ]
                        }
                     }
                  ]
               }
            }
         }
      },
      {
         "label":"Create in last 3 months",
         "equery":"{\"range-query\":{\"type\":\"dateTime\",\"json-property\":\"lastModifiedAt\",\"value\":new Date(new Date().setMonth(new Date().getMonth()-3)),\"range-operator\":\"GE\"}}"
      }
   ]
}
