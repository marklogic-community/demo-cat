<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width">
    <base href="/">
    <!-- Place favicon.ico and apple-touch-icon.png in the root directory -->
    <link rel="shortcut icon" href="/images/favicon.png" />
    <!-- build:css(.tmp) styles/all.css -->
    <link rel="stylesheet" href="styles/main.css" />
    <!-- endbuild -->
  </head>
  <body ng-app="demoCat" ng-controller="SearchCtrl">
    <nav class="navbar navbar-default" role="navigation">
      <div class="navbar-header">
        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#demo-cat-navbar-collapse-1">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
        <a class="navbar-brand" href="/">Demo Cat</a>
      </div>
      <div class="collapse navbar-collapse" id="demo-cat-navbar-collapse-1">
        <ul class="nav navbar-nav">
          <li ng-show="model.user.authenticated"><form action="/create"><button class="btn btn-danger btn-sm btn-create">Create</button></form></li>
        </ul>
        <div class="navbar-form navbar-left">
          <ml-remote-input template="fa"></ml-remote-input>
        </div>
        <div ml-user username="model.user.name" password="model.user.password" authenticated="model.user.authenticated" login="login(username, password)" logout="logout()" loginerror="model.user.loginError"/>
      </div><!-- /.navbar-collapse -->
</nav>


    <!-- Add your site or application content here -->
    <div ng-show="!model.user.authenticated" class="not-logged-in">
      <div class="alert alert-danger col-md-4 login-error" role="alert">Please log in to see content</div>
    </div>
    <div class="container content" ng-view=""></div>
    <footer class="footer">
      <div class="container">
        <p class="text-muted pull-left">Copyright &copy; 2014 MarkLogic Corporation. All Rights Reserved.</p>
        <img src="/images/marklogic-poweredby.png" class="pull-right"/>
      </div>
    </footer>

    <!-- build:js scripts/plugins.js -->
    <script src="bower_components/jquery/jquery.js"></script>
    <script src="bower_components/lodash/dist/lodash.min.js"></script>
    <script src="bower_components/angular/angular.js"></script>
    <script src="bower_components/angular-route/angular-route.js"></script>
    <script src="bower_components/angular-cookies/angular-cookies.js"></script>
    <script src="bower_components/ckeditor/ckeditor.js"></script>
    <script src="bower_components/ng-ckeditor/ng-ckeditor.js"></script>
    <script src="bower_components/angular-bootstrap/ui-bootstrap-tpls.min.js"></script>
    <script src="bower_components/angular-sanitize/angular-sanitize.min.js"></script>
    <script src="bower_components/angular-bootstrap/ui-bootstrap-tpls.min.js"></script>
    <script src="bower_components/ml-utils/ml-utils.min.js"></script>
    <script src="bower_components/ml-common-ng/dist/ml-common-ng.min.js"></script>
    <script src="bower_components/ml-search-ng/dist/ml-search-ng.js"></script>
    <script src="bower_components/ml-search-ng/dist/ml-search-ng-tpls.min.js"></script>
    <script src="bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
    <!-- endbuild -->

    <!-- build:js scripts/scripts.js -->
    <script src="scripts/app.js"></script>
    <script src="user/user.js"></script>
    <script src="user/user-dir.js"></script>
    <script src="user/user-srv.js"></script>
    <script src="search/search.js"></script>
    <script src="search/search-ctrl.js"></script>
    <script src="common/common.js"></script>
    <script src="scripts/directives/add-comment.js"></script>
    <script src="scripts/directives/autocomplete.js"></script>
    <script src="scripts/services/features.js"></script>
    <script src="scripts/services/domains.js"></script>
    <script src="scripts/services/demo.service.js"></script>
    <script src="scripts/controllers/demo.js"></script>
    <script src="scripts/controllers/create.js"></script>
    <script src="scripts/controllers/profile.js"></script>
    <!-- endbuild -->
  </body>
</html>
